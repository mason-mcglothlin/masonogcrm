  <connectionStrings>
    <add name="SqlServerLocalDb" providerName="System.Data.SqlClient" connectionString="Server=(localdb)\mssqllocaldb;AttachDbFilename=|DataDirectory|OgCRMDB.mdf;Integrated Security=true;" />
    <!-- Unimportant password below checked into source code control -->
    <add name="SqlServerAzure" providerName="System.Data.SqlClient" connectionString="Server=tcp:ogcrm.database.windows.net,1433;Database=OgCRM;User ID=masonsa@ogcrm;Password=nosefang1!;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
